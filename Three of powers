

#include <iostream>
#include<bits/stdc++.h>
#include<string>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
   int n,i;
	    cin>>n;
	 string p;
	 cin>>p;
	    int count=0;
	    if ( p == "1" ||  p =="10")
	    cout<<"NO"<<endl;
	    else
	    {
	        for(i=0;i<n;i++){
	            if(p[i]=='1')
	            count++;
	        }
	        if(count<=3)
	        {
	            cout<<"YES"<<endl;
	        }
	        else
	        cout<<"NO"<<endl;
	    }
}
    return 0;
}
