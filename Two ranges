#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int a,b,c,d;
	    int count=0,i;
	    cin>>a>>b>>c>>d;
	  for(i=1;i<100;i++)
	  {
	      if(i>=a&& i<=b)
	      {
	          count++;
	      }
	      else if(i>=c&& i<=d)
	      {
	          count++;
	      }
	  }
	  cout<<count<<endl;
	}
	return 0;
}
